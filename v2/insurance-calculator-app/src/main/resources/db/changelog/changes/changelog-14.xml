<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.2.xsd">


    <changeSet id="38" author="Fistedar">
        <createTable tableName="tc_age_coefficient">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true"/>
            </column>
            <column name="age_from" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="age_to" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="coefficient" type="decimal(10,2)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="39" author="Fistedar">
        <sql>
            INSERT INTO tc_age_coefficient(age_from, age_to, coefficient)
            values (0, 9, 5),
                   (10, 17, 10),
                   (18, 39, 20),
                   (40, 64, 30),
                   (65, 120, 50)
        </sql>
    </changeSet>

</databaseChangeLog>